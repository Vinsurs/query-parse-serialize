{
    "compilerOptions": {
        "target": "ES2015",
        "module": "ESNext",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "declaration": true,
        "emitDeclarationOnly": true,
        "moduleResolution": "Node",
        "outDir": "dist",
    },
    "include": ["src/**/*.ts"]
}