{
  "name": "query-parse-serialize",
  "version": "1.0.0",
  "description": "A library for parsing and serializing query strings in JavaScript",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vinsurs/query-parse-serialize.git"
  },
  "main": "dist/query-parse-serialize.umd.js",
  "module": "dist/query-parse-serialize.es.js",
  "types": "dist/index.d.ts",
  "unpkg": "dist/query-parse-serialize.umd.js",
  "type": "module",
  "files": [
    "dist"
